{
    "audio": "pipewire",
    "bootloader": "systemd-bootctl",
    "filesystem": "btrfs",
    "gfx_driver": "Nvidia",
    "harddrives": [
        "/dev/nvme0n1"
    ],
    "swap": true,
    "hostname": "m-desktop",
    "kernels": [
        "linux"
    ],
    "keyboard-language": "us",
    "mirror-region": "Worldwide",
    "nic": {
        "NetworkManager": true,
    },
    "ntp": true,
    "packages": ["bash", "git", "neovim", "gamemode"],
    "profile": "sway",
    "sys-encoding": "utf-8",
    "sys-language": "en_US",
    "timezone": "US/Eastern",
}
